<div id="modal" class="modal fade" role="dialog">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title"><%= t('mongoid.attributes.course_class.registared_students') %></h4>
      </div><!--//modal-header-->
          <div class="modal-body">
            <table id="course-class-students-table" class="datatable table table-hover table-striped" data-source="<%= students_course_class_path(@course_class, format: :json) %>">
              <thead>
              <tr>
                <th><%= t 'mongoid.attributes.user.fullname' %></th>
                <th class="text-center"><%= t 'mongoid.attributes.student.stc' %></th>
                <th class="text-center"><%= t 'mongoid.attributes.student.semester' %></th>
              </tr>
              </thead>
              <tbody>
              </tbody>
            </table>

          </div><!--//modal-body-->
          <div class="modal-footer">
            <a class="btn btn-warning pull-right" data-dismiss="modal"><%= t('btn.cancel') %></a>
          </div><!--//modal-footer-->
    </div><!--//modal-content-->
  </div><!--//modal-dialog-->
</div>